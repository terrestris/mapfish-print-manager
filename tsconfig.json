{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": ["es2017", "dom", "webworker"],
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "outDir": "./dist",
    "noEmit": false,
    "strict": false,
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "ol": ["node_modules/@hanreev/types-ol/ol"],
      "ol/*": ["node_modules/@hanreev/types-ol/ol/*"]
    }
  },
  "include": [
    "src"
  ]
}
