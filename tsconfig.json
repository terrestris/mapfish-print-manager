{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": ["es2017", "dom", "webworker"],
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "noEmit": false,
    "strict": true,
    "noImplicitAny": true,
    "baseUrl": ".",
    "esModuleInterop": true
  },
  "include": [
    "src"
  ]
}
