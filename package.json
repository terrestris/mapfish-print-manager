{
  "name": "@terrestris/mapfish-print-manager",
  "version": "2.0.0",
  "description": "An interface to easily communicate with the MapFish Print module",
  "author": "terrestris GmbH & Co. KG <info@terrestris.de>",
  "contributors": [
    {
      "name": "Daniel Koch",
      "email": "koch@terrestris.de",
      "url": "https://github.com/dnlkoch"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/terrestris/mapfish-print-manager"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist/* ./build/*",
    "lint": "eslint --ext js src/ spec/",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "start": "webpack-dev-server --config webpack.development.config.js",
    "build:examples": "npm run clean && webpack --config webpack.production.config.js",
    "build:dist": "npm run clean && babel src --out-dir dist",
    "prepublishOnly": "npm run build:dist",
    "release": "np --no-yarn && git push https://github.com/terrestris/mapfish-print-manager.git master --tags"
  },
  "dependencies": {
    "js-logger": "1.4.1",
    "lodash": "4.17.11",
    "parse-color": "1.0.0",
    "parse-css-font": "3.0.2"
  },
  "peerDependencies": {
    "ol": "~5.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.0",
    "babel-jest": "23.0.1",
    "babel-loader": "8.0.2",
    "babel-plugin-import": "1.9.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "canvas-prebuilt": "1.6.5-prerelease.1",
    "eslint": "5.0.0",
    "jest": "23.0.0",
    "jest-fetch-mock": "1.6.6",
    "np": "3.0.4",
    "ol": "5.2.0",
    "rimraf": "2.6.2",
    "webpack": "4.20.0",
    "webpack-cli": "3.1.1",
    "webpack-dev-server": "3.1.9",
    "webpack-merge": "4.1.4"
  }
}
