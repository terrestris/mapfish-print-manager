{
  "name": "@terrestris/mapfish-print-manager",
  "version": "6.5.1",
  "description": "An interface to easily communicate with the MapFish Print module",
  "author": "terrestris GmbH & Co. KG <info@terrestris.de>",
  "contributors": [
    {
      "name": "Daniel Koch",
      "email": "koch@terrestris.de",
      "url": "https://github.com/dnlkoch"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/terrestris/mapfish-print-manager"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean:dist": "rimraf ./dist/*",
    "clean:examples": "rimraf ./build/examples/*",
    "clean:docs": "rimraf ./build/docs/*",
    "clean": "npm run clean:dist && npm run clean:examples && npm run clean:docs",
    "lint": "eslint --ext js ./src/ ./spec/",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run typecheck && npm run lint",
    "test": "jest --maxWorkers=4",
    "test:watch": "jest --watchAll",
    "typecheck": "tsc --pretty --noEmit true --emitDeclarationOnly false",
    "start": "webpack-dev-server --config ./webpack.development.config.js",
    "build:examples": "npm run clean:examples && webpack --config ./webpack.production.config.js",
    "build:dist": "npm run clean:dist && babel src --out-dir dist && tsc --pretty",
    "build:docs": "npm run clean:docs && typedoc",
    "prepublishOnly": "npm run build:dist && npm run build:docs",
    "release": "np --no-yarn",
    "postpublish": "node ./tasks/update-gh-pages.js"
  },
  "dependencies": {
    "@terrestris/eslint-config-typescript-react": "^2.0.0",
    "js-logger": "^1.6.1",
    "lodash": "^4.17.21",
    "parse-color": "^1.0.0",
    "parse-css-font": "^4.0.0",
    "query-string": "^7.1.1",
    "url-parse": "^1.5.10"
  },
  "peerDependencies": {
    "ol": "^7"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@hanreev/types-ol": "^7",
    "@types/jest": "^29.2.3",
    "@webpack-cli/serve": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-import": "^1.13.5",
    "canvas": "^2.10.2",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.28.0",
    "eslint-plugin-jsdoc": "^39.6.2",
    "gh-pages": "^4.0.0",
    "jest": "^29.3.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.3.1",
    "jest-fetch-mock": "^3.0.3",
    "np": "^7.6.2",
    "ol": "^7",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.4.1",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
